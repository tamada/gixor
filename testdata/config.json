{
    "base-path": "boilerplates",
    "repositories": [
        {
            "url": "https://github.com/github/gitignore.git",
            "repo-name": "gitignore",
            "owner": "github",
            "name": "default",
            "path": "default"
        }, {
            "url": "https://github.com/tamada/gitignore.git",
            "repo-name": "gitignore",
            "owner": "tamada",
            "name": "tamada",
            "path": "tamada"
        }, {
            "name": "htamada",
            "url": "https://gitlab.com/htamada/gitignore.git",
            "repo-name": "gitignore",
            "owner": "htamada",
            "path": "htamada"
        }
    ],
    "aliases": [
        {
            "name": "os-list",
            "description": "Alias of linux, windows and macos",
            "boilerplates": [
                "default/linux",
                "default/windows",
                "default/macos"
            ]
        }, {
            "name": "my-default",
            "description": "Alias of my default settings",
            "boilerplates": [
                "alias/os-list",
                "default/visualstudiocode",
                "default/emacs",
                "default/vim"
            ]
        }
    ]
}
